<div class="container d-flex">
     <div class="users-main">
          <% @users.each do |user| %>
          <%= user.email %>
          <%= link_to 'See Profile', user_path(user) %>
          <% end %>
     </div>
          <div id="map"
               style="width: 100%; height: 600px;"
               data-markers="<%= @markers.to_json %>"
               data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
          </div>
</div>
